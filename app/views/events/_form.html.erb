<%= form_with model: event do |form|%>
  <div>
    <%= form.label :title%><br>
    <%= form.text_field :title%>
  </div>
  <div>
    <%= form.label :location%><br>
    <%= form.text_field :location%>
  </div>
  <div>
    <%= form.label :date%><br>
    <%= form.text_field :date%>
  </div>
  <div>
    <%= form.submit%>
  </div>
<% end%>